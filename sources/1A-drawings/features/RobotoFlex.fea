feature liga {
    sub f f i by f_f_i;
    sub f f i j by f_f_ij;
    sub f f l by f_f_l;
    sub f i by fi;
    sub f l by fl;
    sub f f by f_f;
} liga;

feature pnum {
    sub zero by zero.prop;
    sub one by one.prop;
    sub two by two.prop;
    sub three by three.prop;
    sub four by four.prop;
    sub five by five.prop;
    sub six by six.prop;
    sub seven by seven.prop;
    sub eight by eight.prop;
    sub nine by nine.prop;
} pnum;



